{
  "_id": "_design/teams",
  "views": {
    "teams": {
      "map": "function (doc) {\n  if (doc.doc_type === \"team\") emit(doc._id, 1);\n}"
    },
    "access": {
      "map": "function (doc) {\n  if(doc.doc_type === \"access\") emit(doc.email.toLowerCase(), 1);\n}"
    },
    "access-team": {
      "map": "function (doc) {\n  if(doc.doc_type === \"access\") emit(doc.team, 1);\n}"
    },
    "access-team-user": {
      "map": "function (doc) {\n  if(doc.doc_type === \"access\" && doc.status === 'accepted') emit([doc.email.toLowerCase(),doc.team], 1);\n}"
    },
    "access-team-invited-user": {
      "map": "function (doc) {\n  if(doc.doc_type === \"access\" && doc.status === 'invited') emit([doc.email.toLowerCase(),doc.team], 1);\n}"
    },
    "election-team": {
      "map": "function (doc) {\n  if(doc.doc_type === \"election\") emit(doc.team, 1);\n}"
    },
    "election-team-id": {
      "map": "function (doc) {\n  if(doc.doc_type === \"election\" && doc.google_id) emit([doc.google_id, doc.team], 1);\n}"
    }
  },
  "language": "javascript",
  "options": {
    "partitioned": true
  }
}
