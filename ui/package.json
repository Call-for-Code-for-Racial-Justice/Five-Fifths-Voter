{
  "name": "five-fifths-voter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 3000 --host localhost",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "deploy": "vue-cli-service build && ibmcloud cf push",
    "ci-check": "yarn format:diff",
    "ci-format": "prettier --write \"src/**/*.{js,vue,scss}\"",
    "format:diff": "prettier --list-different \"src/**/*.{js,vue,scss}\"",
    "postinstall": "cd .. && husky install",
    "test:a11y": "vue-cli-service test:unit --testPathPattern=a11y"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-0",
    "@carbon/grid": "^10.13.0",
    "@carbon/icons": "^10.19.0",
    "@carbon/icons-vue": "^10.14.0",
    "@carbon/themes": "^10.20.0",
    "@carbon/vue": "^2.34.1",
    "amplitude-js": "^7.3.1",
    "axios": "^0.21.1",
    "carbon-components": "^10.21.0",
    "core-js": "^2.6.5",
    "google-maps-api-loader": "^1.1.1",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "markdown-it": "^12.2.0",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.12",
    "vue-amplitude": "^1.0.3",
    "vue-flag-icon": "^1.0.6",
    "vue-i18n": "^8.25.0",
    "vue-plugin-load-script": "^1.3.2",
    "vue-router": "^3.4.6",
    "vuejs-countdown": "^0.2.1",
    "webpack": "^4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^7.0.4",
    "node-sass": "^4.9.0",
    "sass-loader": "^9",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "engines": {
    "node": "14.x"
  },
  "resolutions": {
    "serialize-javascript": "^3",
    "node-forge": "^0.10",
    "yargs-parser": "^18"
  }
}
